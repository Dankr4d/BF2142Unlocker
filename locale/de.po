msgid "HOST_SETTINGS"
msgstr "Einstellungen"

msgid "HOST_MODS"
msgstr "Mods:"

msgid "HOST_MODS_TOOLTIP"
msgstr "Choose the mod the server should run with"

msgid "HOST_GAME_MODE"
msgstr "Spielmodus:"

msgid "HOST_GAME_MODE_TOOLTIP"
msgstr ""
"Selection of available gameplay modes.\n"
"Available maps depend on the chosen mode."

msgid "HOST_BOT_SKILL"
msgstr "Bot Fähgikeit:"

msgid "HOST_BOT_SKILL_TOOLTIP"
msgstr "Controlls the difficulty of the bots"

msgid "HOST_TICKET_RATIO"
msgstr "Ticket verhältnis"

msgid "HOST_TICKET_RATIO_TOOLTIP"
msgstr "Tickets for each team in %"

msgid "HOST_SPAWN_TIME"
msgstr "Spawn zeit:"

msgid "HOST_SPAWN_TIME_TOOLTIP"
msgstr "Time to wait before one can re-enter the battle if killed"

msgid "HOST_ROUNDS_PER_MAP"
msgstr "Runen pro Karte:"

msgid "HOST_ROUNDS_PER_MAP_TOOLTIP"
msgstr ""
"Number of rounds to play each map before switching to the next one in the "
"list"

msgid "HOST_BOT_AMOUNT"
msgstr "Bot anzahl:"

msgid "HOST_BOT_AMOUNT_TOOLTIP"
msgstr "Total number of all bots on the map"

msgid "HOST_MAX_PLAYERS"
msgstr "Maximale Spieler"

msgid "HOST_MAX_PLAYERS_TOOLTIP"
msgstr ""
"Maximum number of human players on the map.\n"
"Humans replace bots if value is lower than \"Bot amount\"."

msgid "HOST_PLAYERS_NEEDED_TO_START"
msgstr "Anzahl benötigter Spieler:"

msgid "HOST_PLAYERS_NEEDED_TO_START_TOOLTIP"
msgstr ""
"Number of human players that need to be connected into the game to make a "
"round start"

msgid "HOST_FRIENDLY_FIRE"
msgstr "Teambeschuss:"

msgid "HOST_FRIENDLY_FIRE_TOOLTIP"
msgstr "Enables friendly fire"

msgid "HOST_SERVER_IP_ADDRESS"
msgstr "Server IP-Adresse:"

msgid "HOST_SERVER_IP_ADDRESS_TOOLTIP"
msgstr ""
"Choose the IP-Address of the login and game-server.\n"
"Default is your computers current IP-Address.\n"
"Give this address to your friends so they can connect to you."

msgid "HOST_MAPS"
msgstr "Karten"

msgid "HOST_SELECTABLE_MAPS_TOOLTIP"
msgstr ""
"List of maps available to play.\n"
"This list is dependend on the gamemode you chose."

msgid "HOST_SELECTED_MAPS_TOOLTIP"
msgstr "List of maps already in your rotation"

msgid "HOST_MAPS_ADD_TOOLTIP"
msgstr "Add map to rotation"

msgid "HOST_MAPS_REMOVE_TOOLTIP"
msgstr "Remove map from rotation"

msgid "HOST_MAPS_MOVE_UP_TOOLTIP"
msgstr "Move map upwards in the rotation"

msgid "HOST_MAPS_MOVE_DOWN_TOOLTIP"
msgstr "Move map downwards in the rotation"

msgid "HOST_TERMINALS"
msgstr "Terminals"

msgid "HOST_LOGIN_UNLOCK_SERVER"
msgstr "Anmelde-/Freischaltserver starten"

msgid "HOST_LOGIN_UNLOCK_SERVER_TOOLTIP"
msgstr "Start only the login-server"

msgid "HOST_HOST"
msgstr "Alle server starten"

msgid "HOST_HOST_TOOLTIP"
msgstr "Start the login and game-server with the selected setting"

msgid "HOST_CANCEL"
msgstr "Abbrechen"

msgid "HOST_CANCEL_TOOLTIP"
msgstr "Stop all servers!"

